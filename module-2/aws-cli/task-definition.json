{
  "family": "mythicalmysfitsservice",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::993906457390:role/MythicalMysfitsCoreStack-EcsServiceRole-19N35IVSC80M6",
  "taskRoleArn": "arn:aws:iam::993906457390:role/MythicalMysfitsCoreStack-ECSTaskRole-1TRH48M0G8P10",
  "containerDefinitions": [
    {
      "name": "MythicalMysfits-Service",
      "image": "993906457390.dkr.ecr.us-east-1.amazonaws.com/mythicalmysfits/service:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "mythicalmysfits-logs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "awslogs-mythicalmysfits-service"
        }
      },
      "essential": true
    }
  ]
}
